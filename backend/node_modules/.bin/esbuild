#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/saiful-islam/Local D/projects/travelBuddyT/backend/node_modules/esbuild/bin/node_modules:/media/saiful-islam/Local D/projects/travelBuddyT/backend/node_modules/esbuild/node_modules:/media/saiful-islam/Local D/projects/travelBuddyT/backend/node_modules:/media/saiful-islam/Local D/projects/travelBuddyT/node_modules:/media/saiful-islam/Local D/projects/node_modules:/media/saiful-islam/Local D/node_modules:/media/saiful-islam/node_modules:/media/node_modules:/node_modules:/media/saiful-islam/Local D/projects/travelBuddyT/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/media/saiful-islam/Local D/projects/travelBuddyT/backend/node_modules/esbuild/bin/node_modules:/media/saiful-islam/Local D/projects/travelBuddyT/backend/node_modules/esbuild/node_modules:/media/saiful-islam/Local D/projects/travelBuddyT/backend/node_modules:/media/saiful-islam/Local D/projects/travelBuddyT/node_modules:/media/saiful-islam/Local D/projects/node_modules:/media/saiful-islam/Local D/node_modules:/media/saiful-islam/node_modules:/media/node_modules:/node_modules:/media/saiful-islam/Local D/projects/travelBuddyT/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
